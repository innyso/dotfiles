[pull]
	rebase = true
[push]
	default = current
[branch]
  autosetupmerge = always
[alias]
  cou = "!f() { git checkout $1 && git branch -u origin/$1; }; f"
  coo = !git fetch && git checkout
  unstage = reset --soft HEAD^
  ls = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate
  st = status
  gr = grep -Ii
